  "description": "create as fast you think",
  "type": "commonjs",
  "main": "./index.js",
  "scripts": {
    "test": "npm test",
    "build": "rm -rf dist && babel src -d dist && cp package.json dist/package.json && cp LICENSE dist/LICENSE && cp README.md dist/README.md",
    "copy": "rm -rf dist && cp -r src dist && cp package.json dist/package.json && cp LICENSE dist/LICENSE && cp README.md dist/README.md",
